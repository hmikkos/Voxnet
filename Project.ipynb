        "    ax.scatter(-x, y, z, zdir='z')\n",
        "\n",
        "\n",
        "def trace(volume):# prend un np maxi array avec des 0 et des 1 et voxelise\n",
        "    fig, ax = plt.subplots(figsize=(10, 10))\n",
        "    ax = fig.gca(projection='3d')\n",
        "    ax.set_aspect('equal')\n",
        "    ax.voxels(volume, edgecolor=\"black\")\n",
        "    plt.show()\n",
        "\n",
        "\n"
